public class Solution {
    public ArrayList<ArrayList<Integer>> combine(int A, int B) {
        ArrayList<ArrayList<Integer>> res = new ArrayList<ArrayList<Integer>>();
        if(A == 1){
            return 
        }
        for(int i = 1; i < A; i++){
            for(int j = i + 1; j < ; j++){
                ArrayList<Integer> temp = new ArrayList<Integer>();
                temp.add(i,j);
                res.add(temp);
            }
        }
        return res;
    }
}
